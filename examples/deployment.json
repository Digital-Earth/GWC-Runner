{
	"variables": {
		"crawler-db-connection-string": "mongodb://pyxis:QgGaZXx4hSMm2tXkZOca655u76QrcG0YP8KcS5xxWv7vxKWkplLPKoEPEvpe1bLcRAz1JqCwLkpfF2WyT691FA==@pyxis.documents.azure.com:10255/?ssl=true&replicaSet=globaldb",
		"gwc-server-start-port": 63000,
		"gwc-geometry-start-port": 63000,
		"gwc-import-start-port": 64000,
		"proxy-port": "1337",
		"gwc-server-count": 4,
		"gwc-geometry-count": 4,
		"gwc-import-count": 1
	},
	"products": {
		"cli": {
			"product": "cli",
			"version": "1.0.1",
			"setup": "pyx.exe config \"-mongo=${crawler-db-connection-string}\""
		},
		"gwc":{
			"product": "gwc",
			"version": "1.0.3"
		},
		"proxy":{
			"product": "proxy",
			"version": "1.0.4",
			"setup": "npm i"
		}
	},
	"services": {
		"cli": {
			"product": "cli",
			"exec": "pyx.exe"
		},
		"gwc": {
			"product": "gwc",
			"exec": "GeoWebCore.exe",
			"args": [
				"-fd=${dataPath}",
				"-cd=${cachePath}"
			]
		},
		"server": {
			"product": "gwc",
			"exec": "GeoWebCore.exe",
			"args": [
				"-fd=${dataPath}",
				"-cd=${cachePath}",
				"http://${ip}:${gwc-server-start-port}+"
			],
			"instancesPerNode": "${gwc-server-count}"
		},
		"geometry": {
			"product": "gwc",
			"exec": "GeoWebCore.exe",
			"args": [
				"-fd=${dataPath}",
				"-cd=${cachePath}",
				"http://${ip}:${gwc-geometry-start-port}+"
			],
			"instancesPerNode": "${gwc-geometry-count}"
		},
		"import": {
			"product": "gwc",
			"exec": "GeoWebCore.exe",
			"args": [
				"-fd=${dataPath}",
				"-cd=${cachePath}",
				"http://${ip}:${gwc-import-start-port}+"
			],
			"instancesPerNode": "${gwc-import-count}"
		},
		"proxy": {
			"product": "proxy",
			"version": "1.0.4",
			"exec": "${nodePath}",
			"env": {
				"PORT": "${proxy-port}"
			},
			"args": [
				"server.js"
			],
			"instancesPerNode": 1
		}
	}
}
		{
			"type": "node",
			"request": "launch",
			"name": "Debug Tests",
			"program": "${workspaceRoot}\\node_modules\\jasmine\\bin\\jasmine.js",
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch DEV UI",
			"program": "${workspaceRoot}\\ggs.js",
			"args": [
				"ui",
				"--local"
			],
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch node",
			"program": "${workspaceRoot}\\ggs.js",
			"args": [
				"serve"
			]
		}
	]
